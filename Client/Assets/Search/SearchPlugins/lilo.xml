<SearchPlugin xmlns="http://www.mozilla.org/2006/browser/search/">
<ShortName>Lilo</ShortName>
<InputEncoding>UTF-8</InputEncoding>
<Image width="16" height="16">data:image/jpeg;base64,</Image>
<Url type="application/x-suggestions+json" method="GET" template="https://api.lilo.org/suggest/suggest">
  <Param name="q" value="{searchTerms}"/>
  <Param name="client" value="opensearch"/>
</Url>
<Url type="text/html" method="GET" template="https://search.lilo.org/searchweb.php">
  <Param name="q" value="{searchTerms}"/>
  <Param name="client" value="ff_ios"/>
</Url>
<SearchForm>https://search.lilo.org/</SearchForm>
</SearchPlugin>
